{
  "name": "webpack-demo",
  "version": "2.0.0",
  "description": "Webpack Demo",
  "main": "index.js",
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "webpack-dev-server --progress",
    "prod:build": "rimraf dist && cross-env NODE_ENV=production webpack -p --progress",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:eslint": "eslint app config test webpack.config.babel.js --ext .js",
    "lint:stylelint": "stylelint \"app/**/*.css\"",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "cross-env NODE_ENV=test yarn test:jest",
    "test:jest": "jest --coverage",
    "test:mocha": "mocha",
    "test:karma": "karma start test/unit/karma.conf.js --single-run",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerxu/webpack-demo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rogerxu/webpack-demo/issues"
  },
  "homepage": "https://github.com/rogerxu/webpack-demo#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "chunk-manifest-webpack-plugin": "^1.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-compat": "^1.0.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.30",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.3.3",
    "webpack-chunk-hash": "^0.4.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
